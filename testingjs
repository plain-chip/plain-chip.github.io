var getParameterByName = function(name, url) {
  if (!url) url = window.location.href;
  name = name.replace(/[\[\]]/g, "\\$&");
  var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
    results = regex.exec(url);
  if (!results) return null;
  if (!results[2]) return "";
  return decodeURIComponent(results[2].replace(/\+/g, " "));
};

if (window.document.domain && window.document.domain.match(/tvg\.com/, "i")) {
  document.domain = "tvg.com";
}
if (
  window.document.domain &&
  window.document.domain.match(/us\.betfair\.com/, "i")
) {
  document.domain = "us.betfair.com";
}
if (window.document.domain && window.document.domain.match(/fanduel\.com/, 'i')) {
  document.domain = 'fanduel.com';
}

window.clientApp =
  getParameterByName("product") == null || getParameterByName("product") == ""
    ? "tvg4"
    : getParameterByName("product");
window.locationContext =
  getParameterByName("location") == null || getParameterByName("location") == ""
    ? "all"
    : getParameterByName("location");

dataLayer = [];

</html>
